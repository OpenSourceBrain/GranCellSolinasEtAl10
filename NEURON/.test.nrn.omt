# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
# Still in development, subject to change without notice!!

target: test.hoc 
engine: NEURON
mep: .test.mep
experiments:
  Current clamp:
    observables:
      spike times:
        file: 
          path: voltage_30.0deg_0.01nA.dat
          columns: [0,1]
          scaling: [1, 1]
        spike detection: 
          method: threshold
          threshold: 0
        tolerance: 0
